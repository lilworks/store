<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DocsStatus" table="docs_status">
    <id name="dstId" type="boolean" column="dst_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dstTag" type="string" column="dst_tag" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dstName" type="string" column="dst_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
