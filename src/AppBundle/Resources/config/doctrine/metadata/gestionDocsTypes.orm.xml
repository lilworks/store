<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="gestionDocsTypes" table="docs_types">
    <id name="dtyId" type="boolean" column="dty_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dtyTag" type="string" column="dty_tag" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dtyName" type="string" column="dty_name" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dtyPrefix" type="string" column="dty_prefix" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
