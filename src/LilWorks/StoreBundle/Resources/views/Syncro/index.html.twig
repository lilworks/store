{% extends 'base.html.twig' %}

{% block body %}

    <h1>
        {% trans %}storebundle.product.syncro{% endtrans %}
    </h1>

    <ul class="list-inline">
        <li>
            <a role="button" class="btn btn-sm btn-secondary" href="{{ path('syncro_stock') }}"><i class="fa fa-cubes"></i> {% trans %}storebundle.product.syncro.stock{% endtrans %}</a>
        </li>
        <li>
            <a role="button" class="btn btn-sm btn-secondary" href="{{ path('syncro_product') }}"><i class="fa fa-cube"></i> {% trans %}storebundle.product.syncro.product{% endtrans %}</a>
