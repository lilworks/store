        <li>
            <a role="button" class="btn btn-sm btn-secondary" href="{{ path('syncro_user') }}"><i class="fa fa-user"></i> {% trans %}storebundle.product.syncro.user{% endtrans %}</a>
        </li>
        <li>
            <a role="button" class="btn btn-sm btn-secondary" href="{{ path('syncro_text') }}"><i class="fa fa-file-text"></i> {% trans %}storebundle.product.syncro.text{% endtrans %}</a>
